{
  "name": "sgcarstrends",
  "version": "4.56.0",
  "description": "Monorepo for SG Cars Trends",
  "author": "Ru Chern Chong <ruchern.chong@sgcarstrends.com>",
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "turbo format",
    "typecheck": "turbo typecheck",
    "start": "turbo start",
    "test": "turbo test",
    "test:coverage": "turbo test:coverage",
    "test:watch": "turbo test:watch",
    "migrate": "turbo migrate",
    "migrate:check": "turbo migrate:check",
    "prepare": "husky",
    "release": "semantic-release",
    "build:web": "turbo run @sgcarstrends/web#build",
    "dev:web": "turbo run @sgcarstrends/web#dev",
    "test:web": "turbo run @sgcarstrends/web#test",
    "lint:web": "turbo run @sgcarstrends/web#lint",
    "typecheck:web": "turbo run @sgcarstrends/web#typecheck",
    "start:web": "turbo run @sgcarstrends/web#start",
    "db:migrate": "turbo run @sgcarstrends/database#migrate",
    "db:migrate:check": "turbo run @sgcarstrends/database#migrate:check",
    "db:generate": "turbo run @sgcarstrends/database#generate",
    "db:push": "turbo run @sgcarstrends/database#push",
    "db:drop": "turbo run @sgcarstrends/database#drop",
    "auth:generate": "turbo run auth:generate"
  },
  "dependencies": {
    "@heroui/navbar": "^2.2.26"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "semantic-release": "^24.0.0",
    "turbo": "2.8.11-canary.12",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "packageManager": "pnpm@10.22.0",
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json,jsonc}": [
      "pnpm biome check --write --no-errors-on-unmatched"
    ]
  }
}
