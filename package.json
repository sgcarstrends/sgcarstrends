{
  "name": "sgcarstrends",
  "version": "4.8.5",
  "description": "Monorepo for SG Cars Trends",
  "author": "Ru Chern Chong <ruchern.chong@sgcarstrends.com>",
  "license": "MIT",
  "scripts": {
    "build": "turbo build --env-mode=loose",
    "deploy:dev": "sst deploy --stage dev",
    "deploy:prod": "sst deploy --stage prod",
    "deploy:staging": "sst deploy --stage staging",
    "dev": "sst dev --stage local",
    "docs:build": "pnpm -F @sgcarstrends/docs build",
    "docs:dev": "pnpm -F @sgcarstrends/docs dev",
    "lint": "turbo lint",
    "migrate": "turbo migrate",
    "migrate:check": "turbo migrate:check",
    "prepare": "husky",
    "release": "semantic-release",
    "test": "turbo test",
    "test:coverage": "turbo test:coverage",
    "test:e2e": "pnpm -F @sgcarstrends/web test:e2e",
    "test:e2e:ui": "pnpm -F @sgcarstrends/web test:e2e:ui",
    "test:watch": "turbo test:watch",
    "web:build": "pnpm -F @sgcarstrends/web build",
    "web:dev": "pnpm -F @sgcarstrends/web dev",
    "web:start": "pnpm -F @sgcarstrends/web start"
  },
  "dependencies": {
    "sst": "3.17.10"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/aws-lambda": "8.10.152",
    "@types/node": "^22.13.9",
    "@vitest/coverage-v8": "^3.2.4",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "semantic-release": "^24.0.0",
    "turbo": "^2.5.6",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.13.1",
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json}": [
      "pnpm biome check --write"
    ]
  }
}
